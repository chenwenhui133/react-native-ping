{
  "name": "@react-native-ohos/react-native-ping",
  "version": "1.4.0-rc.1",
  "description": "Highly accurate ICMP Ping controller for React Native",
  "main": "index.js",
  "types": "index.d.ts",
  "harmony": {
    "alias": "react-native-ping",
    "redirectInternalImports": true,
    "autolinking": {
      "etsPackageClassName":"RNReactNativePingPackage",
      "cppPackageClassName":"RNCPingPackage",
      "cmakeLibraryTargetName": "rnoh_ping",
      "ohPackageName": "@react-native-ohos/react-native-ping"
    }
  },
  "scripts": {
    "harmony": "react-native bundle-harmony --dev",
    "start": "react-native start",
    "test": "yarn validate:eslint && yarn validate:typescript",
    "validate:eslint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "validate:typescript": "tsc --project ./ --noEmit",
    "codegen-lib": "react-native codegen-lib-harmony --no-safety-check --npm-package-name react-native-ping --cpp-output-path ./harmony/ping/src/main/cpp/generated --ets-output-path ./harmony/ping/src/main/ets/generated --turbo-modules-spec-paths ./src/internal/NativeRNCping.ts --arkts-components-spec-paths ./src"
  },
  "keywords": [
    "react-native",
    "ping",
    "react-native-ping"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RoJoHub/react-native-ping"
  },
  "author": "tiancailuohao@gmail.com",
  "license": "MIT",
  "peerDependencies": {
    "react-native": ">=0.41.2"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/bob": "^0.17.1",
    "@react-native/eslint-config": "^0.77.1",
    "@react-native/metro-config": "^0.77.1",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.3.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "3.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^8.2.1",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.8.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "^0.77.1",
    "react-native-macos": "^0.77.1",
    "react-native-test-app": "^2.5.32",
    "react-native-web": "^0.17.7",
    "react-native-windows": "^0.72.18",
    "react-test-renderer": "18.3.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^17.4.7",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^6.2.1",
    "typescript": "^5.2.2",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.3"
  }
}
